{
  "name": "@cowprotocol/cow-app-data",
  "version": "0.1.0",
  "description": "CowProtocol AppData",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^20.17.31",
    "@types/semver-sort": "^0.0.1",
    "eslint": "^8.48.0",
    "jest": "^29.4.2",
    "jest-fetch-mock": "^3.0.3",
    "json-schema-ref-parser": "^9.0.9",
    "json-schema-to-typescript": "^10.1.5",
    "microbundle": "^0.15.1",
    "prettier": "^2.7.1",
    "semver-sort": "^1.0.0",
    "ts-node": "^10.8.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "cross-fetch": "^3.1.5",
    "ipfs-only-hash": "^4.0.0",
    "json-stringify-deterministic": "^1.0.8"
  },
  "peerDependencies": {
    "cross-fetch": "^3.x",
    "ipfs-only-hash": "^4.x",
    "multiformats": "^9.x"
  }
