{
  "name": "@cowprotocol/cow-app-data",
  "version": "0.1.0",
  "description": "CowProtocol AppData",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**",
    "schemas/**"
  ],
  "scripts": {
    "compile": "tsx src/scripts/compile.ts",
    "prebuild": "npm run compile",
    "build": "tsup src/index.ts --format esm,cjs --dts --external multiformats",
    "dev": "npm run compile && tsup src/index.ts --format esm,cjs --watch --dts --external multiformats",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist/* && rm -rf src/generatedTypes/* && rm -rf schemas/"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^20.17.31",
    "@types/semver-sort": "^0.0.1",
    "jest": "^29.4.2",
    "jest-fetch-mock": "^3.0.3",
    "json-schema-ref-parser": "^9.0.9",
    "json-schema-to-typescript": "^10.1.5",
    "tsup": "^7.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@cowprotocol/common": "workspace:*",
    "ajv": "^8.11.0",
    "cross-fetch": "^3.1.5",
    "ipfs-only-hash": "^4.0.0",
    "json-stringify-deterministic": "^1.0.8",
    "multiformats": "^9.6.4",
    "semver-sort": "^1.0.0"
  },
  "peerDependencies": {
    "cross-fetch": "^3.x",
    "ipfs-only-hash": "^4.x",
    "multiformats": "^9.x"
  }
}
